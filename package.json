{
	"name": "sveltekit_template",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "npm run build && wrangler dev",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test",
		"lint": "eslint .",
		"db:push": "drizzle-kit push --config=drizzle.local.config.ts",
		"db:generate": "drizzle-kit generate --config=drizzle.local.config.ts",
		"db:migrate": "drizzle-kit migrate --config=drizzle.local.config.ts",
		"db:cf-push": "drizzle-kit push --config=drizzle.d1.config.ts",
		"db:studio": "drizzle-kit studio",
		"cf:deploy": "npm run build && npx wrangler deploy",
		"cf:preview": "npm run build && npx wrangler dev",
		"cf-typegen": "wrangler types ./src/worker-configuration.d.ts",
		"ba:generate": "npx @better-auth/cli@latest generate --config ./src/lib/betterauth/auth.ts",
		"ba:migrate": "npx @better-auth/cli@latest migrate --config ./src/lib/betterauth/auth.ts"
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.0",
		"@eslint/js": "^9.39.1",
		"@playwright/test": "^1.56.1",
		"@sveltejs/adapter-cloudflare": "^7.2.5",
		"@sveltejs/kit": "^2.50.0",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@types/better-sqlite3": "^7.6.13",
		"@types/node": "^22",
		"@vitest/browser-playwright": "^4.0.10",
		"drizzle-kit": "^0.18.1",
		"drizzle-orm": "^0.44.7",
		"eslint": "^9.39.1",
		"eslint-plugin-svelte": "^3.13.0",
		"globals": "^16.5.0",
		"playwright": "^1.56.1",
		"svelte": "^5.43.8",
		"svelte-check": "^4.3.4",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.47.0",
		"vite": "^7.2.2",
		"vite-plugin-devtools-json": "^1.0.0",
		"vitest": "^4.0.10",
		"vitest-browser-svelte": "^2.0.1",
		"wrangler": "^4.60.0"
	},
	"dependencies": {
		"@lucide/lab": "^0.1.2",
		"@lucide/svelte": "^0.562.0",
		"better-auth": "^1.4.3",
		"better-sqlite3": "^12.4.1",
		"zod": "^4.3.2"
	}
}
